{
  "name": "@chris-hammersley/family-chart-mongodb",
  "version": "1.0.0",
  "description": "Family tree visualization with MongoDB integration",
  "keywords": [
    "family-chart",
    "family tree",
    "d3",
    "mongodb",
    "mongoose"
  ],
  "homepage": "https://github.com/chris-hammersley/family-chart",
  "license": "MIT",
  "author": {
    "name": "Chris Hammersley"
  },
  "main": "dist/@chris-hammersley/family-chart-mongodb.cjs",
  "exports": {
    ".": "./dist/@chris-hammersley/family-chart-mongodb.cjs",
    "./dist/styles/family-chart.css": "./dist/styles/family-chart.css"
  },
  "unpkg": "dist/family-chart.min.js",
  "module": "dist/family-chart.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chris-hammersley/family-chart.git"
  },
  "files": [
    "dist/**/*.cjs",
    "dist/styles/**",
    "README.md",
    "README_1.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "build": "node build.cjs",
    "build:cjs": "rollup -c rollup.config.mjs",
    "build:all": "npm run build && npm run build:cjs",
    "test": "yarn run cypress open --project tests",
    "dev": "vite",
    "prepublishOnly": "npm run build:all"
  },
  "sideEffects": false,
  "dependencies": {
    "d3": "6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "cypress": "^9.1.0",
    "http-server": "^14.1.1",
    "rollup": "^4.45.0",
    "rollup-plugin-terser": "7",
    "vite": "^6.3.5"
  }
}
