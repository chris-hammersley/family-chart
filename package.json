{
  "name": "@yourusername/family-chart-mongodb", 
  "version": "1.0.0",
  "description": "Family tree visualization with MongoDB integration",
  "keywords": [
    "family-chart",
    "family tree",
    "d3",
    "mongodb",
    "mongoose"
  ],
  "homepage": "https://github.com/yourusername/family-chart",
  "license": "MIT",
  "author": {
    "name": "Your Name"
  },
  "main": "dist/family-chart.js",
  "unpkg": "dist/family-chart.min.js",
  "module": "dist/family-chart.esm.js",
  "exports": {
    ".": {
      "require": "./dist/family-chart.js",
      "import": "./dist/family-chart.esm.js"
    },
    "./styles/*": "./dist/styles/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/family-chart.git"
  },
  "files": [
    "dist/**/*.js",
    "dist/styles/**",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "build": "node build.js",
    "build:cjs": "rollup -c rollup.config.cjs.js",
    "build:all": "npm run build && npm run build:cjs",
    "test": "yarn run cypress open --project tests",
    "dev": "vite",
    "prepublishOnly": "npm run build:all"
  },
  "sideEffects": false,
  "dependencies": {
    "d3": "6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "cypress": "^9.1.0",
    "http-server": "^14.1.1",
    "rollup": "2",
    "rollup-plugin-terser": "7",
    "vite": "^6.3.5"
  }
}
